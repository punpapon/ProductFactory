.login_box {
	webkit-box-shadow: 4px 4px 18px -1px rgba(177,177,177,1);
    -moz-box-shadow: 4px 4px 18px -1px rgba(177,177,177,1);
    box-shadow: 4px 4px 18px -1px rgba(177,177,177,1);
    border-color: transparent;
}

#preloader {
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	background-color: #fefefe;
	z-index: 99;
	height: 100%;
}
#status {
	width: 32px;
	height: 32px;
	position: absolute;
	left: 60%;
	top: 300px;
	background-image: url(../images/shared/ajax-loader1.gif);
	background-repeat: no-repeat;
	background-position: center;
	margin: 0 auto;
}

